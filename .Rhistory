library(tidyverse)
library(tidyverse)
library(dplyr)
library(dplyr)
library(ggplot2)
DVpattern <- read_csv("DevBio_Patterning.csv")
View(DVpattern)
DVpattern <- DVpattern %>% select(1:3)
DVpattern <- DVpattern %>%
rename(phenotype_category = 'Category_of_Phenotype')
DVpattern <- DVpattern %>%
rename(phenotype = 'Dorsalized_or_Ventralized')
Dorsal <- DVpattern %>%
filter(phenotype == "dorsalized")
Ventral <- DVpattern %>%
filter(phenotype == "ventralized")
Dorsal %>% group_by(phenotype_category) %>% tally
Ventral %>% group_by(phenotype_category) %>% tally
Dorsal %>% group_by(Treatment) %>% tally
Ventral %>% group_by(Treatment) %>% tally
#NOTE: do not run this block of code as it is just a template
library(ggplot2)
ggplot(data, aes(x =, y = ))
ggplot(data = DVpattern, aes(x = Treatment)) +
geom_bar()
ggplot(DVpattern, aes(x = Treatment, fill = phenotype_category)) +
geom_bar()
DVpattern1 <- DVpattern %>%
mutate(phenotype_category = as.factor(phenotype_category)) %>%
mutate(phenotype_category =
fct_relevel(phenotype_category, "C5", "C4", "C3", "C2", "C1", "wild-type", "V5", "V4", "V3", "V2", "V1"))
ggplot(DVpattern1, aes(x = Treatment, fill = phenotype_category)) +
geom_bar()
ggplot(DVpattern1, aes(x = Treatment, fill = phenotype_category)) +
geom_bar(position = "fill") +
labs(y = "Proportion")
